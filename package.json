    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/typeorm": "^7.1.5",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^2.25.2",
    "cache-manager": "^3.6.0",
    "class-validator": "^0.13.2",
